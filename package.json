{
  "name": "sumo-logger",
  "version": "2.0.5",
  "description": "Sumo Logic Javascript SDK for sending logs to a HTTP Source endpoint.",
  "main": "lib/sumoLogger.js",
  "scripts": {
    "release": "np",
    "http-server": "grunt http-server:dev &",
    "test": "nyc mocha",
    "cover": "npm run test && nyc report --reporter=lcov",
    "lint": "eslint src/sumoLogger.js src/formatDate.js test/**/*.js",
    "build": "babel -d lib src",
    "test:browser": "npm run http-server && open https://127.0.0.1:8282/jasminetest/TrackerSpecRunner.html",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "acorn": "^6.0.5",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-plugin-node": "^8.0.1",
    "grunt": "^1.0.3",
    "grunt-contrib-jasmine": "^2.0.3",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-http-server": "^2.1.0",
    "gulp-babel": "^8.0.0",
    "mocha": "^5.2.0",
    "np": "^3.1.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SumoLogic/js-sumo-logger.git"
  },
  "keywords": [
    "sumo-logger",
    "jslogger",
    "sumologic",
    "logs",
    "javascript"
  ],
